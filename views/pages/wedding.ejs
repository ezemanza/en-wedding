              </div>
            </div>
          </div>
        </div>

        <!-- SEARCH INVITATION FORM -->
        <div v-else-if="invitation.error || !invitation.fetched" class="row">
          <ajax-form action="getInvitation" :syncing.sync="syncing" :cloud-error.sync="cloudError"
            @submitted="invitationFound" :handle-parsing="handleParsingFindForm">
            <div class="col-md-offset-3 col-md-6 text-center">
              <div id="email" class="form-group">
                <label for="inputemail"><%= __('LET\'S FIND FOR YOUR INVITATION') %></label>
                <input type="email" class="form-control ajax-input" id="inputemail" placeholder="EMAIL"
                  v-model.trim="findInvitationFormData.email">
                <div class="invalid-feedback" v-if="formErrors.email"><%= __('Please enter a valid email address.') %>
                </div>
              </div>
            </div>

            <div class="col-md-12 text-center">
              <p class="invalid-feedback large" v-if="cloudError">
                <small><%= __('An error occured. Please try again.') %></small></p>
              <div class="form-group">
                <ajax-button-template :syncing="syncing"><%= __('FIND') %></ajax-button-template>
              </div>
            </div>
          </ajax-form>
        </div>

        <!-- CONFIRMATION FORM -->
        <div v-else-if="!invitation.confirmed" class="row">

          <ajax-form action="confirmInvitation" :syncing.sync="syncing" :cloud-error.sync="cloudError"
            @submitted="invitationConfirmed" :handle-parsing="handleParsingConfirmForm">

            <div class="col-md-6">
              <div class="form-group">
                <label for="inputemail"><%= __('SELECT THE GUESTS COMING') %></label>
                <div class="ajax-input ajax-checkbox">
                  <div class="checkbox" v-for="guest in invitation.data.guests">
                    <label class="btn btn-primary no-icon" data-toggle="buttons" @click="handleGuests(guest.id)">
                      <input type="checkbox" :value="guest.id">
                      {{ guest.fullName }}
                    </label>
                  </div>
                </div>
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <label for="inputmessage"><%= __('LEAVE YOUR MESSAGE') %></label>
                <textarea id="inputmessage" class="form-control ajax-input" rows="14"
                  v-model.trim="confirmFormData.message"></textarea>
              </div>
            </div>

            <div class="col-md-12 text-center">
              <p class="invalid-feedback large" v-if="cloudError">
                <small><%= __('An error occured. Please try again.') %></small></p>
              <div class="form-group">
                <ajax-button-template :syncing="syncing"><%= __('SUBMIT') %></ajax-button-template>
              </div>
            </div>
          </ajax-form>

        </div>

        <div v-else class="row">
          <div class="col-md-12 text-center">
            <p>
              <%= __('Thank you for the confirmation!') %>
            </p>
          </div>
        </div>
      </div>

