        <div v-if="invitation.pending" class="row">
          <div class="col-md-12 text-center">
            <div class="alignment">
              <div class="v-align center-middle">
                <!-- LEFT HEART -->
                <div class="heart-animation">
                  <i class="de-icon-heart"></i>
                </div>
                <!-- RIGHT HEART -->
                <div class="heart-animation-reverse">
                  <i class="de-icon-heart"></i>
